.sv {
  display: flex;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(74, 74, 74, 1);
}
.sv-title {
  flex: none;
  margin-right: 10rpx;
}
.sv-title::after {
  content: ':';
}
.sv-content {
  flex: auto;
}
.sv.sv-wrap {
  height: auto;
}
.sv.sv-wrap .sv-content {
  word-break: break-all;
}
